{
    "name": "dantleech/maestro",
    "description": "Co-ordinate and manage micro packages",
    "license": "MIT",
    "type": "phpactor-extension",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],

    "require": {
        "phpactor/console-extension": "^0.1.1",
        "phpunit/phpunit": "^8.0",
        "phpstan/phpstan": "^0.11.4",
        "webmozart/path-util": "^2.3",
        "phpactor/config-loader": "^0.1.0",
        "amphp/amp": "^2.1",
        "amphp/process": "^1.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpactor/test-utils": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Maestro\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maestro\\Tests\\": "tests/"
        }
    },
    "extra": {
        "phpactor.extension_class": "Phpactor\\Extension\\Maestro\\MaestroExtension",
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
