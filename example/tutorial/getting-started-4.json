{
    "args": {
        "vars": {
            "animal": "cow",
            "greeting": "moo"
        }
    },
    "nodes": {
        "maestrophp/example-math": {
            "args": {
                "version": "1.0.0"
            },
            "prototype": "example"
        },
        "maestrophp/example-science": {
            "args": {
                "version": "0.1.0"
            },
            "prototype": "example"
        }
    },
    "prototypes": {
        "example": {
            "type": "package_init",
            "args": {
                "url": "https://github.com/$PACKAGE_NAME"
            },
            "nodes": {
                "readme": {
                    "type": "template",
                    "args": {
                        "path": "templates/readme.md.twig",
                        "targetPath": "README.md"
                    }
                },
                "tag": {
                    "type": "tag",
                    "depends": ["readme"]
                },
                "survey": {
                    "type": "survey",
                    "depends": ["tag"]
                }
            }
        }
    }
}

